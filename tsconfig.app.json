{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
      "outDir": "../../dist/out-tsc",
      "types": [],
      "paths": {
        "@/*": ["src/app/*"],
        "@/core/*": ["src/app/core/*"],
        "@/shared/*": ["src/app/shared/*"]
      }
  },
  "angularCompilerOptions": {
    "skipLibCheck": true,
    "enableIvy": false
  },
  "files": ["src/main.ts"],
  "include": ["src/**/*.d.ts"]
}
